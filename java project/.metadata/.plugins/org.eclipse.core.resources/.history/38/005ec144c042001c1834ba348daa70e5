package project2;

public class Salary {
	private double sum;
	private	int month;
	private	double tax;
	public Salary(double a, int b, double c) {
	}
	public void setSum() {
		this.sum = a;
	}
	public void setMonth() {
		this.month = b;
	}
	public void setTax() {
		this.tax = c;
	}
	public double getSum() {
		return sum;
	}
	public int getMonth() {
		return month;
	}
	public double getTax() {
		return tax;
	}
}
